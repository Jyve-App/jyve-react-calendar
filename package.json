{
  "name": "jyve-react-calendar",
  "version": "0.1.0",
  "private": true,
  "main": "dist/build.js",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rimraf dist",
    "clean:examples": "rimraf examples/dist",
    "examples": "npm run clean:examples && webpack-dev-server --config examples/webpack.config.js --mode development",
    "test": "npm run standard",
    "standard": "standard \"**/*.js\"",
    "bump": "npx standard-version"
  },
  "dependencies": {
    "antd": "^3.11.2",
    "axios": "^0.18.0",
    "axios-cache-adapter": "^2.1.1",
    "date-fns": "^1.29.0",
    "emotion": "^9.2.12",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-emotion": "^9.2.12",
    "react-big-calendar": "^0.20.2",
    "react-day-picker": "^7.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.1",
    "eslint-config-standard-react": "^7.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "husky": "^1.1.3",
    "node-sass": "^4.10.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "peerDependencies": {
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test"
    }
  }
